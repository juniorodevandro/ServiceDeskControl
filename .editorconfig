[*.cs]

# CS8600: Conversão de literal nula ou possível valor nulo em tipo não anulável.
dotnet_diagnostic.CS8600.severity = none

# CA1805: Não inicializar desnecessariamente
dotnet_diagnostic.CA1805.severity = silent

# CS8601: Possível atribuição de referência nula.
dotnet_diagnostic.CS8601.severity = silent

# CS8625: Não é possível converter um literal nulo em um tipo de referência não anulável.
dotnet_diagnostic.CS8625.severity = silent

# CS8602: Desreferência de uma referência possivelmente nula.
dotnet_diagnostic.CS8602.severity = silent

# CS8604: Possível argumento de referência nula.
dotnet_diagnostic.CS8604.severity = silent

# CS8622: A nulidade de tipos de referência no tipo de parâmetro não corresponde ao delegado de destino (possivelmente devido a atributos de nulidade).
dotnet_diagnostic.CS8622.severity = silent

# CS8618: O campo não anulável precisa conter um valor não nulo ao sair do construtor. Considere declará-lo como anulável.
dotnet_diagnostic.CS8618.severity = silent
